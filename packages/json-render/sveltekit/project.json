{
  "name": "json-render-sveltekit",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/json-render/sveltekit/src",
  "projectType": "library",
  "tags": ["scope:infrastructure", "scope:ui"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm svelte-check --tsconfig ./tsconfig.svelte-check.json --threshold error",
        "cwd": "packages/json-render/sveltekit"
      },
      "cache": true
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish --provenance",
        "cwd": "packages/json-render/sveltekit"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "packages/json-render/sveltekit/dist",
        "main": "packages/json-render/sveltekit/src/index.ts",
        "tsConfig": "packages/json-render/sveltekit/tsconfig.lib.json",
        "configFile": "packages/json-render/sveltekit/vite.config.ts",
        "assets": [],
        "generatePackageJson": false
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true,
        "configFile": "packages/json-render/sveltekit/vite.config.ts"
      }
    },
    "svelte-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-check --tsconfig ./tsconfig.svelte-check.json --threshold error",
        "cwd": "packages/json-render/sveltekit"
      },
      "cache": true
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/json-render/sveltekit"
      },
      "cache": true
    }
  }
}
